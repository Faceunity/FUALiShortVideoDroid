<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_selected="true">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/aliyun_video_color_effect_bg_color" />
            <size
                android:width="10dp"
                android:height="10dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <size android:height="10dp" android:width="10dp"/>
            <solid android:color="@color/aliyun_video_color_effect_item_bg"/>
        </shape>
    </item>
</selector>